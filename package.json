    "babel-preset-es2015": "^6.13.1",
    "babel-preset-react": "^6.11.1",
    "parallelshell": "^2.0.0",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "webpack": "^1.13.1"
  },
  "devDependencies": {
    "babel-preset-stage-1": "^6.13.0",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-sass": "^2.3.2",
    "gulp-sourcemaps": "^1.6.0",
